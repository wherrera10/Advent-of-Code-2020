isabbrev(s, t) = (length(s) <= length(t)) && (s == t[1:length(s)])

println(isabbrev("as",  "ast"))


println(isabbrev("asb",  "ast"))

println(isabbrev("astw",  "ast"))


function indict(d, s, ref)
    if length(s) <= length(ref)) && (s == ref[1:length(s)])
        for i in length(s):-1:1
            if haskey(d, s[1:i])
                return d[s[1:i]
            end
        end
    end
    return "*error*")
end








		
